<div class="component carousel tt-carousel tt-banner-t1" data-properties='{"autoplayTime":4000}' id="flux">
    <!-- Swiper -->
    <div class="swiper-banner swiper-container">
        <div class="swiper-wrapper">

            <div class="swiper-slide">
                <div class="spotlight caption-video">


                    <div class="banner-image">
                        <video id="my-video" class="video-js vjs-big-play-button" preload="auto" width="100%" height="100%" data-setup='{"muted":true, "controls":true }' >
                            <source src="../needreplace/media/Project/ToyotaTheme/Common/images/spotlights/Toyota_May18_Animate.mp4"
                                    type=video/mp4>

                        </video>
                    </div>
                </div>
            </div>
            <div class="swiper-slide">
                <div class="spotlight caption-center"
                     style="background-image:url('../needreplace/media/Project/ToyotaTheme/Common/images/spotlights/background-image.png') ">


                    <div class="caption">
                        {{#is language "en"}}
                            <p><strong>THE 2018 C-HR TOYOTA</strong></p>
                            <h2><a href="#.">safety<br/>
                                redefined</a></h2>
                        {{else}}
                            <p><strong>THE 2018 C-HR تويوتا</strong></p>
                            <h3><a href="#.">السلامة <br/>
                                بمفهوم جديد</a></h3>
                        {{/is}}
                    </div>

                    <!-- condtional -->
                    <div class="caption-btn">
                        <a href="vehicle-overview.html" class="btn btn-primary">
                            {{#is language "en"}}
                                Explore now
                            {{else}}
                                اكتشف الآن
                            {{/is}}
                        </a>
                    </div>
                    <!-- condtional / -->

                    <div class="banner-image">
