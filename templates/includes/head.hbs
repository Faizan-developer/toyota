<meta charset="UTF-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge"/>
<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0"/>
<link rel="shortcut icon" href="../needreplace/media/Project/ToyotaTheme/Common/images/logos/favicon.ico"/>
<title>
    {{#is language "en"}}
        {{title_en}}
    {{else}}
        {{title_ar}}
    {{/is}}
</title>

{{#is production "true"}}
    {{#is language "en"}}
        <link rel="stylesheet" href="../needreplace/media/Project/ToyotaTheme/Common/css/app.min.css?v={{version}}">
    {{else}}
        <link rel="stylesheet" href="../needreplace/media/Project/ToyotaTheme/Common/css/app-ar.min.css?v={{version}}">
    {{/is}}
{{else}}
    <link rel="stylesheet" href="../needreplace/media/Project/ToyotaTheme/Common/css/reset.css?v={{version}}">

    {{#is language "ar"}}
        <link rel="stylesheet"
              href="../needreplace/media/Project/ToyotaTheme/Common/css/style_arabic.css?v={{version}}">
        <link rel="stylesheet" href="../needreplace/media/Project/ToyotaTheme/Common/css/style_ar.css?v={{version}}">
    {{else}}
        <link rel="stylesheet" href="../needreplace/media/Project/ToyotaTheme/Common/css/style.css?v={{version}}">
    {{/is}}


{{/is}}




