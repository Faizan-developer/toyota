<div class="row">
    <div class="component link-list tt-links-box no-border">
        <ul class="row">
            <li class="col-md-4 col-lg-3">
                <div class="tt-car-item">
                    <div class="image">
                        <img src="../needreplace/media/Project/ToyotaTheme/Common/images/items/vans.png"
                             alt="" class="front">
                        <img src="../needreplace/media/Project/ToyotaTheme/Common/images/items/vans.png"
                             alt="" class="hover"></div>
                    <div class="title notranslate"><h3 class=" notranslate">2017 Prius</h3></div>
                    <div class="pricing">
                        <span class="label"> {{#is language "en"}}
                            {{text_en_starting}}
                        {{else}}
                            {{text_ar_starting}}
                        {{/is}}</span>
                        <div class="price-tag notranslate"><h4>85,900 SR

                        </h4>
                        </div>
                    </div>
                    {{> _item_overview_links }}
                </div>
            </li>
            <li class="col-md-4 col-lg-3">
                <div class="tt-car-item">
                    <div class="image">
                        <img src="../needreplace/media/Project/ToyotaTheme/Common/images/items/vans.png"
                             alt="" class="front">
                        <img src="../needreplace/media/Project/ToyotaTheme/Common/images/items/vans.png"
                             alt="" class="hover"></div>
                    <div class="title notranslate"><h3 class="notranslate">2017 Prius</h3></div>
                    <div class="pricing">
                        <span class="label"> {{#is language "en"}}
                            {{text_en_starting}}
                        {{else}}
                            {{text_ar_starting}}
                        {{/is}}</span>
                        <div class="price-tag notranslate"><h4>85,900 SR

                        </h4>
                        </div>
                    </div>
                    {{> _item_overview_links }}
                </div>
            </li>
        </ul>
    </div>
</div>